{
  "expo": {
    "name": "Amara",
    "slug": "amara",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/icon.png", // Temporarily commented out to allow build to pass
    "userInterfaceStyle": "light",
    "scheme": "amara-v3",
    "splash": {
      // "image": "./assets/splash.png", // Temporarily commented out
      "resizeMode": "contain",
      "backgroundColor": "#0E1C36"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amara",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      // "adaptiveIcon": { // Temporarily commented out
      //   "foregroundImage": "./assets/adaptive-icon.png",
      //   "backgroundColor": "#0E1C36"
      // },
      "package": "com.amara",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "93b88633-cc8e-4d0b-ae28-1c2575f3bb66"
      }
    }
  }
}